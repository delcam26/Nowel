{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "module":"ESNext",
        "moduleResolution":"Node",
        "strict":true,
        "jsx": "preserve",
        "resolveJsonModule":true,
        "isolatedModules":true,
        "esModuleInterop":true,
        "forceConsistentCasingInFileNames":true,
        "skipLibCheck":true,
        "outDir":"./dist"
    },
    "include": ["src"],
    "references":[]
    }
